{
  "name": "react-future-components",
  "private": false,
  "version": "1.0.1",
  "description": "Modern UI components for reactjs",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && postcss src/components/Button.css -o dist/Button.css && vite build",
    "prepare": "npm run build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itss0urav/react-future-components.git"
  },
  "keywords": [
    "react-future-components"
  ],
  "author": "itss0urav",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itss0urav/react-future-components/issues"
  },
  "homepage": "https://github.com/itss0urav/react-future-components#readme",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "postcss-cli": "^11.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1"
  }
}
